import array as groupdata

scores = [26, 49, 98, 87, 62, 75]

var = groupdata.array('i', scores )
print("Array: ")
for i in (var):
  print(i, end = " ")

print("\nOdd Numbers in Array: ")
for i in scores:
  if i % 2 == 0:
    pass
  else:
    print(i, end = " ")

print("\nSummation of Odd Numbers: ")
def odd_num(scores):
  return sum([i for i in scores if i % 2 == 1])
scores = [26, 49, 98, 87, 62, 75]
print(odd_num(scores))
